<package>
	<id>com.d8xa.vantaye-extension</id>
	<name>Vantaye Extension</name>
	<version>1.0.1-1</version>
	<compatibility>
		<firmware>
			<miniOS>7.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>com.anemonetheming.anemone</package>
        <package>com.spark.snowboard</package>
	</dependencies>
	<shortDescription>An inofficial extension of Surenix's Vantaye theme.</shortDescription>
	<descriptionlist>
		<description>This is an inofficial extension of Surenix's theme "Vantaye".</description>
